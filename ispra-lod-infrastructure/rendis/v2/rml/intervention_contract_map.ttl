    rr:objectMap [
      rr:parentTriplesMap :PlaceCollection
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ispra-top:hasLocation;
    rr:objectMap [
      rr:parentTriplesMap :InterventionPlace
    ]
  ];

