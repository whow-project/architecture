:DatasetISPRA
    rml:logicalSource :CSV;
        rr:subjectMap [
            rr:constant "https://dati.isprambiente.it/ld/{{dataset}}";
            rr:class ispra-top:Dataset
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:template  "{{dataset.capitalize()}} Dataset";
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate dcterms:description;
        rr:objectMap [
            rr:template "Repertorio Nazionale degli interventi per la Difesa del Suolo";
            #rr:language "it"
        ]
    ].

:InterventionCode

  rml:logicalSource :CSV;
  
  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/uniqueidentifier/00104_{I_idIntervento}";
    rr:class ispra-top:UniqueIdentifier
  ];
 

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "I_idIntervento";
      rr:language "it"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "I_idIntervento";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ispra-top:hasIdentifierSchema;
    rr:objectMap [
      rr:parentTriplesMap <#IspraIdentifierSchema>
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate ispra-top:identifier;
    rr:objectMap [
      rml:reference "I_idIntervento"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ispra-top:hasCreator;
    rr:objectMap [
      rr:constant "https://dati.isprambiente.it/ld/common/organisation/001";
      rr:termType rr:IRI
    ]
  ].
  

 <#IspraIdentifierSchema>

   rml:logicalSource :CSV;

   rr:subjectMap [
    rr:constant "https://dati.isprambiente.it/ld/common/identifierschema/00104";
    rr:class ispra-top:IdentifierSchema
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:constant "Schema ISPRA identificativi interventi {{dataset.capitalize()}}";
      rr:language "it"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:constant "ISPRA identifier schema of {{dataset.capitalize()}} interventions";
      rr:language "en"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate ispra-top:identifier;
    rr:objectMap [
      rml:constant "04"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate ispra-top:isIssuedBy;
    rr:objectMap [
      rr:constant "https://dati.isprambiente.it/ld/common/organisation/001";
      ]
    ] .


:Intervention
  rml:logicalSource :CSV;
  
  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/procedure/{I_idIntervento}";
