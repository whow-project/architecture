#===============================================================================
#SENSOR MODEL

:SensorModelData
	rml:logicalSource :CSV_SENSOR_MODELS;

	rr:subjectMap [
        	rr:template "https://dati.isprambiente.it/ld/{{dataset}}/sensormodel/{ID}";
	        rr:class inspire-mf:SensorModel
    	];


	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
            		rml:reference "ID";
	    		rr:language "it"
        	]
    	];

	rr:predicateObjectMap [
        	rr:predicate rdfs:label;
	        rr:objectMap [
        		rml:reference "ID";
			rr:language "en"
		]
    	];

	rr:predicateObjectMap [
        	rr:predicate ispra-top:name;
	        rr:objectMap [
        		rml:reference "NAME"
		]
    	];

	rr:predicateObjectMap [
		rr:predicate inspire-mf:hasSensorType;
		rr:objectMap [
			rr:parentTriplesMap :SensorType
		]
	];

	rr:predicateObjectMap [
        	rr:predicate ispra-top:hasManufacturer;
        	rr:objectMap [
            		rr:parentTriplesMap :Manufacturer
        	]
    	];

	rr:predicateObjectMap [
        	rr:predicate ispra-top:isClassifiedBy;
        	rr:objectMap [
			rml:reference "WMO";
			rr:termType rr:IRI
        	]
    	].

