@prefix : <https://dati.isprambiente.it/ld/rml/sensors_map.ttl#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix crml: <http://w3id.org/stlab/crml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ispra-place: <https://dati.isprambiente.it/ontology/place/> .
@prefix ispra-top: <https://dati.isprambiente.it/ontology/top/> .
@prefix inspire-mf: <https://dati.isprambiente.it/ontology/inspire-mf/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geosparql: <http://www.opengis.net/ont/geosparql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .


:CSV_SENSORS
	rml:source "{{ csv_sensors }}" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".

:CSV_SENSOR_MODELS
	rml:source "{{ csv_sensor_models }}" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".


:CSV_SENSOR_DEPICTIONS
	rml:source "{{ csv_sensor_depictions }}" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".


#:Place
#	rml:logicalSource :CSV_SENSORS;
#
#	rr:subjectMap [
#		rr:template "https://dati.isprambiente.it/ld/place/municipality/00201_#{CODE_PLACE}"
#	].


:Platform
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
		rr:template "https://dati.isprambiente.it/ld/{{dataset}}/platform/{STAT_CODE}_{NETWORK}_tide_gauge";
	].


:Sensor
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
		rr:template "https://dati.isprambiente.it/ld/{{dataset}}/sensor/{STAT_CODE}_{NETWORK}_{SENS_TYPE_EN}{SENS_ID}";
		rr:class inspire-mf:Sensor 
	];

        rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "{SENS_TYPE_IT} posizionato nella piattaforma {STAT_CODE}";
			rr:language "it"
      		]
    	];

        rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "{SENS_TYPE_EN} placed on the platform {STAT_CODE}";
			rr:language "en"
      		]
    	];


	rr:predicateObjectMap [
		rr:predicate inspire-mf:isHostedBy;
		rr:objectMap [
			rr:parentTriplesMap :Platform
      		]
    	];

	rr:predicateObjectMap [
		rr:predicate inspire-mf:hasSensorModel;
		rr:objectMap [
			rr:parentTriplesMap :SensorModelData;
			rr:joinCondition [
				rr:child "SENSOR_MODEL"; 
				rr:parent "ID"
			]
		]
	];

	rr:predicateObjectMap [
		#rr:predicate inspire-mf:hasPeriod;
		rr:predicate inspire-mf:hasSystemCapability;
		rr:objectMap [
			rr:parentTriplesMap :SensorPeriodQP
		]
	];

	
#	rr:predicateObjectMap [
#		rr:predicate ispra-top:isClassifiedBy;
#		rr:objectMap [
#			rr:parentTriplesMap :SensorModelType
#		]
#	];

	rr:predicateObjectMap [
		rr:predicate inspire-mf:hasSystemDeployment;
		rr:objectMap [
			rr:parentTriplesMap :MeasurementHeightQP
		]
	];

	rr:predicateObjectMap [
        	rr:predicate inspire-mf:hasSystemCapability;
        	rr:objectMap [
	    		rr:parentTriplesMap :SamplingFrequencyQP
        	]
    	].
    
#===============================================================================
#SENSOR PROPERTIES


:SamplingFrequencyQP

	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
        	rr:template "https://dati.isprambiente.it/ld/{{dataset}}/systemcapability/{STAT_CODE}_{NETWORK}_{SENS_TYPE_EN}{SENS_ID}_sampling_frequency_{SENSOR_SAMPLING_FREQUENCY}_hz";
        	rr:class inspire-mf:SystemCapability
    ];
        
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "Frequenza di campionamento del sensore {SENS_TYPE_IT} della rete {{dataset.upper()}} {NETWORK}";
	    		rr:language "it"
      ]
    ];
     
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
        		rr:template "Sampling frequency of the sensor of the {SENS_TYPE_EN} of the {{dataset.upper()}} {NETWORK}";
	    		rr:language "en"
      		]
    	];
     
     
	rr:predicateObjectMap [
      		rr:predicate ispra-top:isParametrisedBy;
		rr:objectMap [
        		rr:parentTriplesMap :SamplingFrequency
      		]
    	];
        
    	rr:predicateObjectMap [
      		rr:predicate ispra-top:hasValue;
      		rr:objectMap [
        		rr:parentTriplesMap :SamplingFrequencyValue
      		]
    	] .


:SamplingFrequency
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
    		rr:template "https://dati.isprambiente.it/ld/common/parameter/sampling_frequency";
    		rr:class ispra-top:Parameter
  	];

  	rr:predicateObjectMap [
    		rr:predicate rdfs:label;
    		rr:objectMap [
      			rr:template "Frequenza di campionamento";
	  		rr:language "it"
      		]
    	];
     
	rr:predicateObjectMap [
    		rr:predicate rdfs:label;
    		rr:objectMap [
      			rr:template "Sampling frequency";
	  		rr:language "en"
      		]
    	];
      
  	rr:predicateObjectMap [
    		rr:predicate owl:sameAs;
    		rr:objectMap [
      			rr:parentTriplesMap :SamplingDBpedia
    		]
  	];

	rr:predicateObjectMap [
    		rr:predicate owl:sameAs;
    		rr:objectMap [
      			rr:parentTriplesMap :SamplingWikidata
    		]
  	] .


:SamplingDBpedia
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
    		rr:template "http://dbpedia.org/resource/Sampling_(signal_processing)"
  	] .

:SamplingWikidata
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
    		rr:template "http://www.wikidata.org/entity/Q210868"
  	] .


:SamplingFrequencyValue

	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
		rr:template "https://dati.isprambiente.it/ld/common/value/{SENSOR_SAMPLING_FREQUENCY}_hz";
		rr:class ispra-top:Value
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "SENSOR_SAMPLING_FREQUENCY";
			rr:language "it"
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "SENSOR_SAMPLING_FREQUENCY";
			rr:language "en"
		]
	];

	rr:predicateObjectMap [
		rr:predicate ispra-top:value;
		rr:objectMap [
			rml:reference "SENSOR_SAMPLING_FREQUENCY";
			rr:datatype xsd:float
		]
	];

    	rr:predicateObjectMap [
        	rr:predicate ispra-top:hasUnitOfMeasure;
        	rr:objectMap [
            		rr:parentTriplesMap :Hertz
        	]
    	].




:SensorPeriodQP

	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
        	rr:template "https://dati.isprambiente.it/ld/{{dataset}}/systemcapability/{STAT_CODE}_{NETWORK}_{SENS_TYPE_EN}{SENS_ID}_averaging_period_{PERIOD}{PERIOD_UNIT}";
        	rr:class inspire-mf:SystemCapability
    ];
        
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "Intervallo di calcolo delle media del sensore {SENS_TYPE_IT} della rete {{dataset.upper()}} {NETWORK}";
	    		rr:language "it"
      ]
    ];
     
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
        		rr:template "Averaging period of the sensor of the {SENS_TYPE_EN} of the {{dataset.upper()}} {NETWORK}";
	    		rr:language "en"
      		]
    	];
     
     
	rr:predicateObjectMap [
      		rr:predicate ispra-top:isParametrisedBy;
		rr:objectMap [
        		rr:parentTriplesMap :SensorPeriod
      		]
    	];
        
    	rr:predicateObjectMap [
      		rr:predicate ispra-top:hasValue;
      		rr:objectMap [
        		rr:parentTriplesMap :SensorPeriodValue
      		]
    	] .


:SensorPeriod
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
    		rr:template "https://dati.isprambiente.it/ld/common/parameter/sampling";
    		rr:class ispra-top:Parameter
  	];

  	rr:predicateObjectMap [
    		rr:predicate rdfs:label;
    		rr:objectMap [
      			rr:template "Campionamento";
	  		rr:language "it"
      		]
    	];
     
	rr:predicateObjectMap [
    		rr:predicate rdfs:label;
    		rr:objectMap [
      			rr:template "Sampling";
	  		rr:language "en"
      		]
    	];
      
  	rr:predicateObjectMap [
    		rr:predicate owl:sameAs;
    		rr:objectMap [
      			rr:constant "http://dbpedia.org/resource/Sampling_(signal_processing)";
			rr:termType rr:IRI
    		]
  	];

	rr:predicateObjectMap [
    		rr:predicate owl:sameAs;
    		rr:objectMap [
      			rr:constant "http://www.wikidata.org/entity/Q210868";
			rr:termType rr:IRI
    		]
  	] .


:SensorPeriodValue

	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
		rr:template "https://dati.isprambiente.it/ld/common/value/p{PERIOD}{PERIOD_UNIT}";
		rr:class ispra-top:Value
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "{PERIOD}{PERIOD_UNIT}";
			rr:language "it"
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "{PERIOD}{PERIOD_UNIT}";
			rr:language "en"
		]
	];

	rr:predicateObjectMap [
		rr:predicate ispra-top:value;
		rr:objectMap [
			rml:reference "PERIOD";
			rr:datatype xsd:float
		]
	];

    rr:predicateObjectMap [
        rr:predicate ispra-top:hasUnitOfMeasure;
        rr:objectMap [
            rr:parentTriplesMap :UnitOfMeasure
        ]
    ].




:MeasurementHeightQP

	rml:logicalSource :CSV_SENSORS;

