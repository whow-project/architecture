  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "TIME";
      rr:language "it"
    ]
  ];
     
  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "TIME";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate core:value;
    rr:objectMap [
