:IspraCode
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/code/{STAT_CODE}";
    rr:class core:Code
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "STAT_CODE";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "STAT_CODE";
      rr:language "it"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate core:code;
    rr:objectMap [
      rml:reference "STAT_CODE"
    ]
  ] .

:StationCollection
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/platformcollection/{CODE_PLACE}_platforms";
    rr:class core:PlatformCollection
  ];

  rr:predicateObjectMap [
    rr:predicate core:hasLocation;
    rr:objectMap [
      rr:parentTriplesMap :#Place
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "Catalogue of the measure stations located in {PLACE_NAME}.";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "Catalogo delle stazioni di misurazione {{dataset|upper}} localizzate a {PLACE_NAME}.";
      rr:language "it"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate core:isPartOf;
    rr:objectMap [
      rr:parentTriplesMap :Catalogue;
    ]
  ].

:Station
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/platform/{STAT_CODE}_{NETWORK}_{STAT_TYPE_EN}";
    rr:class inspire-mf:Platform #, inspire:MonitoringFacility
  ];

    rr:predicateObjectMap [
      rr:predicate inspire-mf:hasPlatformType;
      rr:objectMap [
        #rr:parentTriplesMap :StationType
        rr:template "https://dati.isprambiente.it/ld/{{dataset}}/platformtype/{STAT_TYPE_EN}";
        rr:termType rr:IRI
      ]
    ];
    
    rr:predicateObjectMap [
    	rr:predicate core:isPartOf;
    	rr:objectMap [
      		rr:parentTriplesMap :StationCollection;
      	]
  	];
        
    rr:predicateObjectMap [
      rr:predicate inspire-mf:hasPlatformModel;
      rr:objectMap [
        rr:parentTriplesMap :StationModelData;
	    rr:joinCondition [
          rr:child "STAT_CODE";
          rr:parent "MODEL" 
        ]
      ]
    ];

    #        rr:predicateObjectMap [
    #            rr:predicate inspire-mf:hasNodeStationModel;
    #            rr:objectMap [
    #                rr:parentTriplesMap <#StationModel>
    #            ]
    #        ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "{STAT_TYPE_EN} part of the {{dataset.upper()}}-{NETWORK} network located near by {PLACE_NAME}";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "{STAT_TYPE_IT} parte della rete {{dataset.upper()}}-{NETWORK} localizzata in prossimitÃ  di {PLACE_NAME}";
      rr:language "it"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate core:hasUniqueIdentifier;
    rr:objectMap [
      rr:parentTriplesMap :IspraCode
    ]
  ];

#  rr:predicateObjectMap [
#    rr:predicate inspire-mf:hosts;
#    rr:objectMap [
#      rr:parentTriplesMap :Sensor>
#  ]
#];

  rr:predicateObjectMap [
    rr:predicate location:long;
    rr:objectMap [
      rr:template "{LONG}"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate location:lat;
    rr:objectMap [
      rr:template "{LAT}"
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicate core:isPartOf;
    rr:objectMap [
      rr:parentTriplesMap :NETWORK
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate core:hasLocation;
    rr:objectMap [
      rr:parentTriplesMap :Feature
    ]
  ].

:StationType
  rml:logicalSource :CSV_STATION_MODELS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/platformtype/{TYPE_EN}";
    rr:class inspire-mf:PlatformType
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "TYPE_EN";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "TYPE_IT";
      rr:language "it"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate owl:sameAs;
    rr:objectMap [
      rr:parentTriplesMap :StationTypeWMO;
      rr:joinCondition [
	rr:child "MODEL";
        rr:parent "MODEL"
      ]
    ]
  ].


:StationTypeWMO

  rml:logicalSource :CSV_STATIONTYPE_WMO;

    rr:subjectMap [
      rr:template "{WMO}"
    ] .

:StationTypeWMOClassification

  rml:logicalSource :CSV_STATIONTYPE_WMO_CLASSIFICATION;
    
  rr:subjectMap [
    rml:reference "WMO"
  ] .

:StationGeometry
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/geometry/{STAT_CODE}_{STAT_TYPE_EN}";
    rr:class location:Geometry
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "Geolocalization of the node station {PLACE_NAME}-{STAT_TYPE_EN} ({{dataset}}-{NETWORK})";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "Geolocalizazione del nodo stazione {PLACE_NAME}-{STAT_TYPE_IT} ({{dataset}}-{NETWORK})";
      rr:language "it"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate location:long;
    rr:objectMap [
      rr:template "{LONG}"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate location:lat;
    rr:objectMap [
      rr:template "{LAT}"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate location:geometry;
    rr:objectMap [
      rr:template "POINT ({LONG} {LAT})";
      rr:datatype "http://www.opengis.net/ont/geosparql#wktLiteral"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate location:hasGeometryType;
    rr:objectMap [
      rr:parentTriplesMap :StationGeometryType
    ]
  ].

:StationGeometryType
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:constant "https://dati.isprambiente.it/ld/common/geometrytype/point";
    rr:class location:GeometryType
  ].


:NETWORK
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/network/{NETWORK}";
    rr:class inspire-mf:Network #, inspire:MonitoringNetwork
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "{{dataset.upper()}} - {NETWORK} Network";
      rr:language "en"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "{{dataset.upper()}} - {NETWORK} Network";
      rr:language "it"
    ]
  ].

