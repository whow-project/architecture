      rr:template "Collezione delle osservazioni {{dataset.upper()}} fatte a {PLACE_NAME}";
      rr:language "it"
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicate core:isPartOf;
    rr:objectMap [
      rr:parentTriplesMap :Catalogue;
    ]
  ] .

