:LevellingQP

    rml:logicalSource :CSV_STATION_MODELS;

    rr:subjectMap [
        rr:template "https://dati.isprambiente.it/ld/{{dataset}}/systemcapability/%eval:station_model_id(*, '{{dataset}}')%_levelling";
