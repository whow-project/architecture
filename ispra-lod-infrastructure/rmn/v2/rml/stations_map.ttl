@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .


:CSV_STATIONS
  rml:source "{{ csv_stations }}" ;
  rml:referenceFormulation ql:CSV;
  crml:separator ";".

:CSV_STATIONTYPE_WMO
    rml:source "{{ csv_wmo }}" ;
    rml:referenceFormulation ql:CSV;
    crml:separator ";".

:CSV_STATIONTYPE_WMO_CLASSIFICATION
    rml:source "{{ csv_wmo_classification }}" ;
    rml:referenceFormulation ql:CSV;
    crml:separator ";".

:CSV_SEAS
    rml:source "{{ csv_seas }}" ;
    rml:referenceFormulation ql:CSV;
    crml:separator ";".

:CSV_STATION_MODELS
  rml:source "{{ csv_station_models }}" ;
  rml:referenceFormulation ql:CSV;
  crml:separator ";".

:Place
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/location/municipality/0201_{CODE_PLACE}"
  ].

:Catalogue
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    #rr:template "https://dati.isprambiente.it/ld/{{dataset}}/collection/{CODE_PLACE}_resources";
    rr:template "https://dati.isprambiente.it/ld/rmn/collection/0201_{CODE_PLACE}";
#    rr:class core:Collection
  ] ;

  rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:template "Collezione di risorse RMN associate a: {PLACE_NAME}";
            rr:language "it"
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:template "RMN resources collection associated with: {PLACE_NAME}";
            rr:language "en"
        ]
    ];

  rr:predicateObjectMap [
    rr:predicate core:hasLocation;
    rr:objectMap [
      rr:parentTriplesMap :Place
    ]
] .



