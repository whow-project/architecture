:Feature
  rml:logicalSource :CSV_STATIONS;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/{{dataset}}/featureofinterest/{STAT_CODE}";
    rr:class inspire-mf:FeatureOfInterest
  ];

  rr:predicateObjectMap [
    rr:predicate location:hasGeometry;
    rr:objectMap [
      rr:parentTriplesMap :FeatureGeometry
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate location:hasNearbyLocation;
    rr:objectMap [
      rr:parentTriplesMap :Place
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate core:isPartOf;
    rr:objectMap [
      rr:parentTriplesMap :Sea;
      rr:joinCondition [
        rr:child "SEA_IT";
        rr:parent "IT"
      ]
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "Area di mare vicino la stazione {{dataset}} di {PLACE_NAME}.";
      rr:language "it"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "Sea area near {PLACE_NAME} {{dataset}} station.";
      rr:language "en"
    ]
  ].

