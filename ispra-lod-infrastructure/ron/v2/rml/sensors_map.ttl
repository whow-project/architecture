#SENSOR MODEL


:SensorModelType
    rml:logicalSource :CSV_SENSOR_MODELS;

    rr:subjectMap [
        rr:template "https://dati.isprambiente.it/ld/{{dataset}}/sensortype/{EN}";
        rr:class inspire-mf:SensorType
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "IT";
	    rr:language "it"
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "EN";
	    rr:language "en"
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate core:specialises;
        rr:objectMap [
            rr:parentTriplesMap :SensorType;
            rr:joinCondition [
                rr:child "ID";
                rr:parent "SENSOR_MODEL"
            ]
        ]
    ].

:SensorModelData
    rml:logicalSource :CSV_SENSOR_MODELS;

    rr:subjectMap [
        rr:template "https://dati.isprambiente.it/ld/{{dataset}}/sensormodel/{ID}";
        rr:class inspire-mf:SensorModel
    ];

    rr:predicateObjectMap [
	rr:predicate core:isClassifiedBy;
        rr:objectMap [
            rml:reference "WMO";
	    rr:termType rr:IRI
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate core:name;
	rr:objectMap [
            rml:reference "NAME"
	]
    ];

    rr:predicateObjectMap [
	rr:predicate inspire-mf:hasSensorType;
	rr:objectMap [
	    rr:parentTriplesMap :SensorModelType
	]
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "ID";
	    rr:language "it"
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "ID";
	    rr:language "en"
        ]
    ] .