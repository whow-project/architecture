#===============================================================================
#SENSOR TYPE

:SensorType
    rml:logicalSource :CSV_SENSOR_TYPES;

    rr:subjectMap [
        rr:template "https://dati.isprambiente.it/ld/common/sensortype/{SENS_TYPE_EN}";
	rr:class inspire-mf:SensorType 
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "SENS_TYPE_EN";
            rr:language "en"
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "SENS_TYPE_IT";
            rr:language "it"
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate core:specialises;
        rr:objectMap [
            rr:parentTriplesMap :SensorTypeBroader;
            rr:joinCondition [
                rr:child "TYPE_EN";
                rr:parent "TYPE_EN"
            ]
        ]
    ].

:SensorTypeBroader
    rml:logicalSource :CSV_SENSOR_SUPERTYPES;

    rr:subjectMap [
    	rr:template "https://dati.isprambiente.it/ld/common/sensortype/{TYPE_EN}";
	rr:class inspire-mf:SensorType 
    ];

    rr:predicateObjectMap [
    	rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "TYPE_EN";
            rr:language "en"
        ]
    ];

    rr:predicateObjectMap [
    	rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "TYPE_IT";
            rr:language "it"
        ]
    ];

    rr:predicateObjectMap [
    	rr:predicate owl:sameAs;
        rr:objectMap [
            rml:reference "DBPEDIA";
            rr:termType rr:IRI
        ]
    ];

    rr:predicateObjectMap [
    	rr:predicate owl:sameAs;
        rr:objectMap [
            rml:reference "WIKIDATA";
            rr:termType rr:IRI
        ]
    ] .


