@prefix : <https://dati.isprambiente.it/ld/rml/sensors_map.ttl#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix crml: <http://w3id.org/stlab/crml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix place: <https://dati.isprambiente.it/ontology/place/> .
@prefix core: <https://dati.isprambiente.it/ontology/core/> .
@prefix inspire-mf: <https://dati.isprambiente.it/ontology/inspire-mf:/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geosparql: <http://www.opengis.net/ont/geosparql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .


:CSV_SENSORS
	rml:source "{{ csv_sensors }}" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".

:CSV_SENSOR_MODELS
	rml:source "{{ csv_sensor_models }}" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".

:CSV_SENSOR_TYPES
	rml:source "{{ csv_sensors_types }}" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".

:CSV_SENSOR_SUPERTYPES
	rml:source "{{ csv_sensors_supertypes }}" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".


:Platform
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
		rr:template "https://dati.isprambiente.it/ld/{{dataset}}/platform/{STAT_CODE}_{STAT_MODEL}_fixed_buoy";
	].

:Sensor
	rml:logicalSource :CSV_SENSORS;

	rr:subjectMap [
		rr:template "https://dati.isprambiente.it/ld/{{dataset}}/sensor/{STAT_CODE}_{STAT_MODEL}_{SENS_TYPE_EN}";
	];

        rr:predicateObjectMap [
	    rr:predicate rdfs:label;
            rr:objectMap [
                rr:template "{SENS_TYPE_IT} ospitato dalla piattaforma {STAT_CODE} della rete {{dataset|upper}}{NETWORK}";
                rr:language "it"
            ]
	];

        rr:predicateObjectMap [
	    rr:predicate rdfs:label;
            rr:objectMap [
                rr:template "{SENS_TYPE_EN} hosted by the platform {STAT_CODE} part of the network {{dataset|upper}}{NETWORK}";
                rr:language "en"
            ]
	];


	rr:predicateObjectMap [
	    rr:predicate inspire-mf:hasSensorModel;
            rr:objectMap [
                rr:template "https://dati.isprambiente.it/ld/{{dataset}}/sensormodel/{SENSOR_MODEL}";
                rr:termType rr:IRI
            ]
	];

        rr:predicateObjectMap [
		rr:predicate inspire-mf:hasSystemCapability;
		rr:objectMap [
			rr:parentTriplesMap :SensorPeriodQP
		]
	];

        rr:predicateObjectMap [
		rr:predicate inspire-mf:hasSystemDeployment;
		rr:objectMap [
			rr:parentTriplesMap :MeasurementHeightQP
		]
	];

        rr:predicateObjectMap [
        	rr:predicate inspire-mf:hasSystemCapability;
        	rr:objectMap [
	    		rr:parentTriplesMap :SamplingFrequencyQP
        	]
    	];

	rr:predicateObjectMap [
		rr:predicate inspire-mf:hasSystemCapability;
		rr:objectMap [
			rr:parentTriplesMap :DirectionalResultQP
		]
	];
	
	rr:predicateObjectMap [
		rr:predicate core:isClassifiedBy;
		rr:objectMap [
			rr:parentTriplesMap :PrimarySensor
		]
	];

        rr:predicateObjectMap [
		rr:predicate inspire-mf:isHostedBy;
		rr:objectMap [
			rr:parentTriplesMap :Platform
      		]
    	] .


#===============================================================================
#PrimarySensor

:PrimarySensor

    rml:logicalSource :CSV_SENSORS;

    crml:condition "df['IS_PRIMARY'].notnull()";

    rr:subjectMap [
        rr:template "https://dati.isprambiente.it/ld/{{dataset}}/concept/%eval:is_primary({IS_PRIMARY})%";
        rr:class core:Concept
    ];
        
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
	rr:objectMap [
	    rr:template "%eval:is_primary({IS_PRIMARY}, 'it')%";
	    rr:language "it"
        ]
    ];
     
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
	rr:objectMap [
	    rr:template "%eval:is_primary({IS_PRIMARY})%";
	    rr:language "en"
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
	rr:objectMap [
	    rr:template "%eval:is_primary({IS_PRIMARY}, 'wmo')%";
	    rr:termType rr:IRI
        ]
    ] .
    
#===============================================================================
#SENSOR PROPERTIES


:DirectionalResultQP

	rml:logicalSource :CSV_SENSORS;

        crml:condition "df['DIRECTIONAL_RESULT'].notnull()";

	rr:subjectMap [
        	rr:template "https://dati.isprambiente.it/ld/{{dataset}}/systemcapability/{STAT_CODE}_{STAT_MODEL}_{SENS_TYPE_EN}_directional_angle_{DIRECTIONAL_RESULT}_deg";
        	rr:class inspire-mf:SystemCapability
    ];
        
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "Angolo direzionale del sensore {SENS_TYPE_IT} della rete {{dataset.upper()}} {NETWORK}";
	    		rr:language "it"
      ]
    ];
     
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
        		rr:template "Directional angle of the sensor of the {SENS_TYPE_EN} of the {{dataset.upper()}} {NETWORK}";
	    		rr:language "en"
      		]
    	];
     
     
	rr:predicateObjectMap [
      		rr:predicate core:isParametrisedBy;
		rr:objectMap [
        		rr:parentTriplesMap :DirectionalResult
      		]
    	];
        
    	rr:predicateObjectMap [
      		rr:predicate core:hasValue;
      		rr:objectMap [
        		rr:parentTriplesMap :DirectionalResultValue
      		]
    	] .


:DirectionalResult
	rml:logicalSource :CSV_SENSORS;

        crml:condition "df['DIRECTIONAL_RESULT'].notnull()";

	rr:subjectMap [
    		rr:template "https://dati.isprambiente.it/ld/common/parameter/directional_angle";
    		rr:class core:Parameter
  	];

  	rr:predicateObjectMap [
    		rr:predicate rdfs:label;
    		rr:objectMap [
      			rr:template "Angolo direzionale";
	  		rr:language "it"
      		]
    	];
     
	rr:predicateObjectMap [
    		rr:predicate rdfs:label;
    		rr:objectMap [
      			rr:template "Directional angle";
	  		rr:language "en"
      		]
    	] .


:DirectionalResultValue

	rml:logicalSource :CSV_SENSORS;

        crml:condition "df['DIRECTIONAL_RESULT'].notnull()";

	rr:subjectMap [
		rr:template "https://dati.isprambiente.it/ld/common/value/{DIRECTIONAL_RESULT}_deg";
		rr:class core:Value
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "{DIRECTIONAL_RESULT} degrees angle";
			rr:language "it"
		]
	];

	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rr:template "{DIRECTIONAL_RESULT} degrees angle";
			rr:language "en"
		]
	];

	rr:predicateObjectMap [
		rr:predicate core:value;
		rr:objectMap [
			rml:reference "DIRECTIONAL_RESULT";
			rr:datatype xsd:float
		]
	];

    	rr:predicateObjectMap [
        	rr:predicate core:hasUnitOfMeasure;
        	rr:objectMap [
            		rr:parentTriplesMap :DegreeAngle
        	]
    	].




