@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix crml: <http://w3id.org/stlab/crml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix location: <https://dati.isprambiente.it/ontology/place/> .
@prefix core: <https://dati.isprambiente.it/ontology/core/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geosparql: <http://www.opengis.net/ont/geosparql#> .




<#RegionsCSV>
  rml:source "location/input/csv/regions.csv" ;
  rml:referenceFormulation ql:CSV;
  crml:separator "\t".


<#DatasetISPRA>
    rml:logicalSource <#RegionsCSV>;

    rr:subjectMap [
        rr:template "https://dati.isprambiente.it/ld/location";
        rr:class core:Dataset
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:template  "location Dataset";
        ]
    ].

<#IstatCode>
  rml:logicalSource <#RegionsCSV>;
  
  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/location/uniqueidentifier/{COD_REG}";
    rr:class core:UniqueIdentifier
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "{COD_REG} (Istat)";
      rr:language "it"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "{COD_REG} (Istat)";
      rr:language "en"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate core:identifier;
    rr:objectMap [
      rml:reference "COD_REG"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate core:isIssuedBy;
    rr:objectMap [
      rr:parentTriplesMap <#IstatOrg>
      ]
    ] . 

 <#IstatOrg>

  rml:logicalSource <#RegionsCSV>;

  rr:subjectMap [
    rr:constant "https://dati.isprambiente.it/ld/location/organisation/istat";
    rr:class core:Organisation
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:constant "Istituto nazionale di statistica ";
      rr:language "it"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:constant "Italian National Institute of Statistics";
      rr:language "en"
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate core:acronym;
    rr:objectMap [
      rr:constant "ISTAT"
      ]
    ] .

   
 <#Regions>
  rml:logicalSource <#RegionsCSV>;
  
  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/location/region/0201_{COD_REG}";
    rr:class location:Region
  ];
  
  rr:predicateObjectMap [
    rr:predicate core:name;
    rr:objectMap [
      rml:reference "DEN_REG";
      rr:datatype xsd:string
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "DEN_REG";
      rr:datatype xsd:string
    ]
  ];
  
  rr:predicateObjectMap [
    rr:predicate location:istat;
    rr:objectMap [
      rml:reference "COD_REG";
      rr:datatype xsd:string
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate core:hasUniqueIdentifier;
    rr:objectMap [
      rr:parentTriplesMap <#IstatCode>
      ]
    ];

  rr:predicateObjectMap [
        rr:predicate core:partOf;
        rr:objectMap [
            rr:parentTriplesMap <#DatasetISPRA>;
        ]
    ];

  rr:predicateObjectMap [
        rr:predicate location:hasCountry;
        rr:objectMap [
            rr:parentTriplesMap <#Nation>;
        ]
  ].

 <#Nation>
  rml:logicalSource <#RegionsCSV>;

  rr:subjectMap [
    rr:template "https://dati.isprambiente.it/ld/location/country/1";
    rr:class location:Country
  ];

  rr:predicateObjectMap [
    rr:predicate core:name;
    rr:objectMap [
      rr:template "Italia";
      rr:datatype xsd:string
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rr:template "Italia";
      rr:datatype xsd:string
      ]
    ];
  rr:predicateObjectMap [
    rr:predicate location:istat;
    rr:objectMap [
      rr:template "1";
      rr:datatype xsd:string
    ]
  ].