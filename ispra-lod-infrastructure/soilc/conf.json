{
	"dataset": "soilc",
	"graph_iri": "https://dati.isprambiente.it/ld/soilc",
	"rdf_dump_file_name": "soilc_{{year}}",
	"rdf_dump_file_serialisation": "ntriples",
	"data_folder": "./soilc/v2/data",
    "dirty_data_folder": "./v2/dirtydata",
    "rml_folder": "./soilc/v2/rml",
	"year": {{year}},
	"mappings": [
		{
            "rml": "indicators_mapV2.ttl",
            "data": [
                {
                    "id": "csv_file",
                    "file": "Nazionale_{{year}}.csv"
                }
            ],
            "variables": [
                {
                    "id": "indicators",
                    "value": "MappingConfiguration.columns_as_tuple('soilc/v2/data/Nazionale_{{year}}.csv', 2)"
                },
                {
                    "id": "YEAR",
                    "value": "'{{year}}'"
                },
                {
                    "id": "COD_PLACE",
                    "value": "'COD'"
                },
                {
                    "id": "NOME_PLACE",
                    "value": "'Nazione'"
                }
            ]
        },
		{
            "rml": "indicators_mapV2.ttl",
            "data": [
                {
                    "id": "csv_file",
                    "file": "Regioni_{{year}}.csv"
                }
            ],
            "variables": [
                {
                    "id": "indicators",
                    "value": "MappingConfiguration.columns_as_tuple('soilc/v2/data/Regioni_{{year}}.csv', 3)"
                },
                {
                    "id": "YEAR",
                    "value": "'{{year}}'"
                },
                {
                    "id": "COD_PLACE",
                    "value": "'COD_REG'"
                },
                {
                    "id": "NOME_PLACE",
                    "value": "'NOME_Regione'"
                }
            ]
        },
        {
            "rml": "indicators_mapV2.ttl",
            "data": [
                {
                    "id": "csv_file",
                    "file": "Province_{{year}}.csv"
                }
            ],
            "variables": [
                {
                    "id": "indicators",
                    "value": "MappingConfiguration.columns_as_tuple('soilc/v2/data/Province_{{year}}.csv', 5)"
                },
                {
                    "id": "YEAR",
                    "value": "'{{year}}'"
                },
                {
                    "id": "COD_PLACE",
                    "value": "'COD_PRO'"
                },
                {
                    "id": "NOME_PLACE",
                    "value": "'NOME_Provincia'"
                }
            ]
        },
        {
            "rml": "indicators_mapV2.ttl",
            "data": [
                {
                    "id": "csv_file",
                    "file": "Comuni_{{year}}.csv"
                }
            ],
            "variables": [
                {
                    "id": "indicators",
                    "value": "MappingConfiguration.columns_as_tuple('soilc/v2/data/Comuni_{{year}}.csv', 7)"
                },
                {
                    "id": "YEAR",
                    "value": "'{{year}}'"
                },
                {
                    "id": "COD_PLACE",
                    "value": "'PRO_COM'"
                },
                {
                    "id": "NOME_PLACE",
                    "value": "'Nome_Comune'"
                }
            ]
        }
	]
}
